      "8xl": {
        "$type": "number",
        "$value": "{scale.12}",
        "$description": "Display sm"
      },
      "9xl": {
        "$type": "number",
        "$value": "{scale.14}",
        "$description": "Display md"
      },
      "10xl": {
        "$type": "number",
        "$value": "{scale.16}",
        "$description": "Display lg"
      }
    }
