      "xs": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "Medium Italic",
          "fontSize": "{typescale.font-size.6xl}",
          "lineHeight": "{typescale.line-height.6xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-display-xs — Use for the smallest display text in tertiary featured content."
      }
