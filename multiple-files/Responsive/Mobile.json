      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.4xl}",
          "lineHeight": "{typescale.line-height.4xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-headline-lg — Use for large section headlines and major content groupings."
