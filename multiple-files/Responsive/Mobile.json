        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.secondary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.2xl}",
          "lineHeight": "150%",
          "paragraphSpacing": "20px",
          "textDecoration": "{text-decoration.line-through}"
