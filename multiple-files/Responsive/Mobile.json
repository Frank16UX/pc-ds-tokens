        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.secondary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.4xl}",
          "lineHeight": "150%",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-price-lg — Use to display current product's PDP."
      },
      "lg-strikethrough": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.secondary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.4xl}",
          "lineHeight": "150%",
          "paragraphSpacing": "20px",
          "textDecoration": "{text-decoration.line-through}"
        },
        "$description": "$mobile-typography-price-lg-strikethrough — Use for display previous product's price on PDP."
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.secondary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.2xl}",
          "lineHeight": "150%",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-price-md — Use to display current product's price on standard product cards."
