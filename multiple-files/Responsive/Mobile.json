  "typography": {
    "display": {
      "xl": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "Medium Italic",
          "fontSize": "{typescale.font-size.10xl}",
          "lineHeight": "{typescale.line-height.10xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-display-xl — Use for the largest display text in hero sections and major headings."
      },
      "lg": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "Medium Italic",
          "fontSize": "{typescale.font-size.9xl}",
          "lineHeight": "{typescale.line-height.9xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-display-lg — Use for large display text in prominent hero sections."
      },
      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "Medium Italic",
          "fontSize": "{typescale.font-size.8xl}",
          "lineHeight": "{typescale.line-height.8xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-display-md — Use for medium display text in featured sections and banners."
      },
      "sm": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "Medium Italic",
          "fontSize": "{typescale.font-size.7xl}",
          "lineHeight": "{typescale.line-height.7xl}",
          "paragraphSpacing": "20px"
        },
        "$description": "$mobile-typography-display-sm — Use for smaller display text in secondary hero sections."
