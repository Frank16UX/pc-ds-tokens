      "md": {
        "$type": "typography",
        "$value": {
          "fontFamily": "{font-family.primary}",
          "fontWeight": "{font-weight.regular}",
          "fontSize": "{typescale.font-size.3xl}",
          "lineHeight": "{typescale.line-height.3xl}",
          "paragraphSpacing": "20px"
