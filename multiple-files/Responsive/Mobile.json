{
  "section-padding": {
    "vertical": {
      "xl": {
        "$type": "number",
        "$value": "{scale.18}",
        "$description": "$mobile-section-padding-vertical-xl — Use for extra large vertical spacing in hero and feature sections."
      },
      "lg": {
        "$type": "number",
        "$value": "{scale.16}",
        "$description": "$mobile-section-padding-vertical-lg — Use for default vertical spacing (top and bottom padding) in major sections."
      },
      "md": {
        "$type": "number",
        "$value": "{scale.12}",
        "$description": "$mobile-section-padding-vertical-md — Use for medium vertical spacing in standard sections."
      },
      "sm": {
        "$type": "number",
        "$value": "{scale.10}",
        "$description": "$mobile-section-padding-vertical-sm — Use for small vertical spacing in compact sections."
      },
      "xs": {
        "$type": "number",
        "$value": "{scale.8}",
        "$description": "$mobile-section-padding-vertical-xs — Use for extra small vertical spacing in tight layouts. Apply to top/bottom padding for adjacent sections with white bg."
      }
    },
    "horizontal": {
      "lg": {
        "$type": "number",
        "$value": "{scale.12}",
        "$description": "$mobile-section-padding-horizontal-lg — Default horizontal padding for main content areas."
      },
      "md": {
        "$type": "number",
        "$value": "{scale.6}",
        "$description": "$mobile-section-padding-horizontal-md — Use for medium horizontal padding in standard containers."
      },
      "sm": {
        "$type": "number",
        "$value": "{scale.6}",
        "$description": "$mobile-section-padding-horizontal-sm — Use for small horizontal padding in narrow containers."
      },
      "xs": {
        "$type": "number",
        "$value": "{scale.4}",
        "$description": "$mobile-section-padding-horizontal-xs — Use for extra small horizontal padding in compact layouts."
      }
    }
  },
